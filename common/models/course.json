{
  "name": "Course",
  "base": "Model",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "instructor": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "students": {
      "type": "hasAndBelongsToMany",
      "model": "Student",
      "foreignKey": "student"
    },
    "course-Ratings": {
      "type": "hasMany",
      "model": "Course-Ratings",
      "foreignKey": "course"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
